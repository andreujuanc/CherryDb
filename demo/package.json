  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "npm-run-all": "^4.1.2",
    "svelte": "^2.0.0",
    "svelte-loader": "^2.9.0",
    "webpack": "^4.7.0"
  }
}
